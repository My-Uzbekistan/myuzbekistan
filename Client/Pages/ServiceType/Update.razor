@page "/service-types/{Id:long}"
@inherits ComputedStateComponent<List<ServiceTypeView>>
@{
    var Model = State.GetValue(Injector);
}

@if (Model.Count > 0)
{
    <Client.Pages.ServiceType._Form OnSubmit="OnSubmit"
                                    Model="Model"
                                    Processing="@Processing"
                                    IsNew="false" />
}
