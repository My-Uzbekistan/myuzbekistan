@page "/merchantcategories/{MerchantCategoryId:long}/merchants/{Id:long}"
@inherits ComputedStateComponent<List<MerchantView>>
@{
    var Model = State.GetValue(Injector);
}

@if (Model.Count > 0)
{
    <Client.Pages.Merchant._Form OnSubmit="OnSubmit"
                                 Model="Model"
                                 MerchantCategoryId="MerchantCategoryId"
                                 Processing="@Processing"
                                 IsNew="false" />
}

