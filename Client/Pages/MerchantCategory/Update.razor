@page "/merchantcategories/{Id:long}"
@inherits ComputedStateComponent<List<MerchantCategoryView>>
@{
    var Model = State.GetValue(Injector);
}

@if(Model.Count > 0)
{
    <Client.Pages.MerchantCategory._Form OnSubmit="OnSubmit"
                                         Model="Model"
                                         Processing="@Processing"
                                         IsNew="false" />
}
